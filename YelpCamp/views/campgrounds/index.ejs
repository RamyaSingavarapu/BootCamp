<% layout('/layouts/boilerplate') %>
    <h1>All Campgrounds</h1>
    <div>
        <a href="/campgrounds/new">Make A New Campground</a>
    </div>
    <ul>
        <% for(let campgrounds of campground ) { %>
            <li>
                <a href="/campgrounds/<%= campgrounds._id %>">
                    <%= campgrounds.title %>
                </a>
            </li>
            <% } %>
    </ul>